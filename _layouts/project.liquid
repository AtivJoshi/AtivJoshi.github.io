---
layout: default
---
<!-- page.html -->
        <div class="post">

          <header class="post-header">
            <h1 class="post-title">{{ page.title }}</h1>
            <!-- <p class="post-description">{{ page.description | markdownify | remove: '<p>' | remove: '</p>'}}</p> -->
          </header>
          <p style="font-size: 150%;" class="post-tags">
              {% if page.code != blank %}
                <!-- &nbsp; &middot; &nbsp; -->
                <a href="{{ page.code }}"><i class="fas fa-code"></i> code </a>
              {%endif%}
    
              {% if page.certi != blank %}
              {% if page.code != blank %} &nbsp; &middot; &nbsp; {%endif%}
                <a href="{{ page.certi }}"> <i class="fas fa-certificate fa-sm"></i> certificate </a>
              {%endif%}
    
              {% if page.report != blank %}
              {% if page.code != blank or page.certi != blank%} &nbsp; &middot; &nbsp; {%endif%}
                <a href="{{ page.code }}"> <i class="fas fa-book fa-sm"></i> report </a>
              {%endif%}
          </p><br>
          <article>
            {{ content }}
          </article>

        </div>